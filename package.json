{
  "name": "readability",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@contentful/app-sdk": "4.12.1",
    "@contentful/f36-components": "4.20.3",
    "@contentful/f36-tokens": "4.0.1",
    "@contentful/field-editor-multiple-line": "^1.1.6",
    "@contentful/field-editor-rich-text": "^3.1.1",
    "@contentful/field-editor-test-utils": "^1.2.2",
    "@contentful/react-apps-toolkit": "1.2.7",
    "@contentful/rich-text-plain-text-renderer": "^15.12.1",
    "@contentful/rich-text-types": "^15.12.1",
    "contentful-management": "10.14.0",
    "emotion": "10.0.27",
    "flesch-kincaid": "github:daveross/flesch-kincaid",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-scripts": "5.0.1"
  },
  "scripts": {
    "start": "cross-env BROWSER=none react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "create-app-definition": "contentful-app-scripts create-app-definition",
    "upload": "contentful-app-scripts upload --bundle-dir ./build",
    "upload-ci": "contentful-app-scripts upload --ci --bundle-dir ./build  --organization-id $CONTENTFUL_ORG_ID --definition-id $CONTENTFUL_APP_DEF_ID --token $CONTENTFUL_ACCESS_TOKEN"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@contentful/app-scripts": "1.1.22",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "12.1.5",
    "@tsconfig/create-react-app": "1.0.2",
    "@types/jest": "29.0.2",
    "@types/node": "18.7.18",
    "@types/react": "18.0.9",
    "@types/react-dom": "18.0.3",
    "cross-env": "7.0.3",
    "typescript": "4.8.3"
  },
  "homepage": "."
}
